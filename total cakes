import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
         System.out.print("Enter number of cakes produced per hour: ");
        int cakesPerHour = scanner.nextInt();
        System.out.print("Enter total number of hours: ");
        int hours = scanner.nextInt();
        int totalCakes = cakesPerHour * hours;

       
        System.out.println("Total cakes produced: " + totalCakes);

     ;
    }
}
